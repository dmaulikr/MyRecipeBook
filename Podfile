  use_frameworks!

  # Pods for MyRacipeBook
  pod 'RealmSwift'
  pod 'Moya-ObjectMapper/RxSwift', :git => 'https://github.com/ivanbruel/Moya-ObjectMapper'
  pod 'Reusable'
  pod 'SVProgressHUD'
  pod 'Kingfisher', '~> 3.0'
  pod 'Reachability', :git => 'https://github.com/ashfurrow/Reachability.git', :branch => 'frameworks'
  pod 'SwifterSwift'
  
  target 'MyRecipeBookTests' do
    inherit! :search_paths
    # Pods for testing
	pod 'Quick'
    pod 'Nimble'
    pod 'ObjectMapper'
  end

end
